{
    "name": "inspiredminds/contao-maker-bundle",
    "description": "This is an extension of the Symfony Maker Bundle, providing autoloading in the Contao Managed Edition as well as additional commands specific to a Contao installation.",
    "keywords": ["contao", "generator", "code generator", "scaffolding", "scaffold"],
    "type": "contao-bundle",
    "homepage": "http://www.inspiredminds.at",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Fritz Michael Gschwantner",
            "homepage": "http://www.inspiredminds.at",
            "email": "fmg@inspiredminds.at",
            "role": "Developer"
        }
    ],
    "support":{
        "email": "fmg@inspiredminds.at",
        "issues": "https://github.com/inspiredminds/contao-maker-bundle/issues",
        "source": "https://github.com/inspiredminds/contao-maker-bundle",
        "forum": "https://community.contao.org/de"
    },
    "require":{
        "php": ">=7.1",
        "contao/core-bundle": "^4.4",
        "symfony/maker-bundle": "^1.0",
        "symfony/yaml": "^3.3 || ^4.1",
        "composer/composer": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "InspiredMinds\\ContaoMakerBundle\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "InspiredMinds\\ContaoMakerBundle\\ContaoManagerPlugin"
    }
}
